{{>header }}

<div class="page-content">

    <div class="row">
        <div class="col-md-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h6 class="card-title">New Accounts Code</h6>

                    <div class="table-responsive">
                        <table id="dataTableExample" class="table">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Phone</th>
                                    <th>Code</th>
                                    <th>Date</th>

                                </tr>
                            </thead>
                            <tbody>
                                {{#each data}}
                                    <tr>

                                        <td>{{ this.User.surname }}
                                            {{ this.User.otherNames }}</td>
                                        <td>{{ this.User.phoneNumber }}</td>
                                        <td>{{ this.code }}</td>

    <td>{{{ formatDate this.createdAt }}}
                                    </td>
                                    </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="provider-details" class="modal fade bd-example-modal-xl" tabindex="-1" role="dialog" aria-labelledby="myExtraLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="card">
                <div class="card-body">
                    <h6 class="card-title">DETAILS</h6>

                    <form method="POST">
                        <div class="row">
                            <div class="col-sm-3">
                                <div class="form-group">
                                    <label for="surname">Surname</label>
                                    <input type="text" class="form-control" id="surname" autocomplete="off" placeholder="Surname" disabled></div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="form-group">
                                        <label for="otherNames">Other names</label>
                                        <input type="text" class="form-control" id="otherNames" autocomplete="off" placeholder="otherNames" disabled></div>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="form-group">
                                            <label for="email">Email address</label>
                                            <input type="email" class="form-control" id="email" placeholder="Email" disabled></div>
                                        </div>
                                        <div class="col-sm-3">

                                            <div class="form-group">
                                                <label for="phoneNumber">Phone number</label>
                                                <input type="phone" class="form-control" id="phoneNumber" placeholder="Phone number" disabled></div>
                                            </div>

                                        </div>

                                        <div class="row">

                                            <div class="col-sm-3">
                                                <div class="form-group">
                                                    <label for="companyName">Name of company</label>
                                                    <input type="text" class="form-control" id="companyName" autocomplete="off" placeholder="Name of company" disabled></div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="form-group">
                                                        <label for="officeLocation">Office location</label>
                                                        <input type="text" class="form-control" id="officeLocation" autocomplete="off" placeholder="Office location(District, Community)" disabled></div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="ghanaPostGps">Ghana Post GPS</label>
                                                            <input type="text" class="form-control" id="ghanaPostGps" autocomplete="off" placeholder="Ghana Post GPS" disabled></div>
                                                        </div>
                                                    </div>

                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            {{>footer }}

                        </div>
                                                <script src="/vendors/core/core.js"></script>

                        <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
                        <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.24/js/jquery.dataTables.js"></script>
                        <script>
                            $(document).ready(function () {
                                $('#dataTableExample').dataTable({"bSort": false});
                            })
                        </script>
                        <!-- core:js -->
                        <!-- endinject -->

                        <!-- plugin js for this page -->
                     
                        <!-- end plugin js for this page -->
                        <!-- inject:js --><script src="/vendors/feather-icons/feather.min.js"> </script>
                        <script src="/js/template.js"></script>
                        <!-- endinject -->
                        <!-- custom js for this page -->
                        <!-- end custom js for this page -->
                        <!-- end custom js for this page -->
                        <script src="/js/custom/transaction-controller.js"> </script>


                    </body>


                </html>
