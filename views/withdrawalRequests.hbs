<div class="container-fluid">

    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">

                    <div class="table-responsive">
                        <table id="example23" class="display nowrap table table-hover table-striped table-bordered"
                            cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>Req ID</th>
                                    <th>Provider Id</th>
                                    <th>Provider name</th>
                                    <th>Provider phone</th>
                                    <th>Cost</th>
                                    <th>MoMo number</th>
                                    <th>Mobile network</th>
                                    <th>Date</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each reqs}}
                                <tr>
                                    <td>{{id}} </td>
                                    <td>{{data.providerCode}}</td>
                                    <td>{{data.providerName}}</td>
                                    <td>{{data.providerNumber}}</td>
                                    <td>{{data.amount}}</td>
                                    <td>{{data.momoNumber}}</td>
                                    <td>{{data.mobileNetwork}}</td>
                                    <td>{{data.date}}</td>
                                    <td><button class="btn btn-success btn-anim tst3" type="button"
                                            onclick="sendMoMo(this)">Disburse</button></td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>

                </div>
